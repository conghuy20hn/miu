{% if data | length > 0 %}
    {% for key,price in data %}
        <div class="item">
            <a href="javascript:void(0)"><img width="90" height="90" alt="" src="{{ price['image_path'] }}"></a>
            <p href="javascript:void(0)" class="multiline-ellipsis-3" style="font-size: 16px; max-height: 61px;">{{ price['name'] }}</p>
            <p style="font-size: 14px; padding-top: 5px;">
                {#<strong>{{ price['price']['title'] }}</strong>#}
                <strong>Gi√°</strong>: <span class="color-01">{{ price['price']['value'] }}</span></p>
        </div>
    {% endfor %}
{% endif %}