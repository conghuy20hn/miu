{% if videos|length>0 %}
    {% for key, video in videos %}
        {% if key%2==0 %}
            <div class="row">
            {% endif %}
            <div class="col-01 item">
                <div class="overlay">
                    <img width="100%" class="img-responsive" src="{{ video.image_path }}">
                    <a class="btn-over" href="{{ url('video/video-detail', {'slug':video.slug}) }}"><i
                            class="glyphicon"></i></a>
                </div>
                <a class="multiline-ellipsis-2" style="max-height: 37px;font-size: 16px" href="{{ url('video/video-detail', {'slug':video.slug}) }}">{{ video.title }}</a>
            </div>
            {% if key%2 !=0 %}
            </div>
        {% endif %}
    {% endfor %}

    {% if url %}
        <div class="mdl-2 ajaxLazyLoad2 articleDetailLoad" container="articleDetailLoad"
             data-url-ajax="{{ url }}"
             {#data-callback-ajax1="sliderVideo"#}
             data-callback-ajax1="cutTextArticleDetailLoad">
        </div>
    {% else %}
        <style type="text/css">
            .loading {
                display: none;
            }
        </style>
    {% endif %}
{% endif %}
