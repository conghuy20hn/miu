/*box-search*/
.popup-search {
	position:fixed;
	width:100%;
	height:100%;
	top:0;
	left:0;
	right:0;
	bottom:0;
	background:rgba(0,0,0,0.8);
	z-index:1031;
	display:none;
}

.box-search {
	margin-top:0;
	background:#000;
	color:#fff;
	float:left;
	width:100%;
	padding:10px 0 6px 0;
	}
	
.box-search .glyphicon{
	color:#fff;
	font-size:1.7em;
	margin:2px 10px 10px 10px;
	}
	
.box-search .glyphicon-refresh{
